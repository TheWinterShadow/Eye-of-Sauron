{
  "uid": "github-actions-history",
  "title": "GitHub Actions - Historical Runs",
  "tags": [
    "github",
    "actions",
    "cicd",
    "loki"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 0,
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Active Projects (30d)",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "count(count by (repo) (count_over_time({job=\"github-actions-collector\", org=~\"$org\"} | json [$__range])))",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "blue"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Healthy Projects",
      "description": "Projects with zero failures in the time range",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "targets": [
        {
          "expr": "count(count by (repo) (count_over_time({job=\"github-actions-collector\", org=~\"$org\"} | json [$__range]))) - count(count by (repo) (count_over_time({job=\"github-actions-collector\", org=~\"$org\"} | json | conclusion=~\"failure|cancelled\" [$__range])))",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Projects with Failures",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 16,
        "y": 0
      },
      "targets": [
        {
          "expr": "count(count by (repo) (count_over_time({job=\"github-actions-collector\", org=~\"$org\"} | json | conclusion=~\"failure|cancelled\" [$__range])))",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 1,
                "color": "red"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      }
    },
    {
      "id": 4,
      "type": "table",
      "title": "Latest Run Per Project",
      "gridPos": {
        "h": 14,
        "w": 24,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "{job=\"github-actions-collector\", org=~\"$org\"} | json",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          },
          "queryType": "range"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left",
            "cellOptions": {
              "type": "auto"
            },
            "inspect": false
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "conclusion"
            },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-text"
                }
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "type": "value",
                    "options": {
                      "success": {
                        "text": "✓ Success",
                        "color": "green",
                        "index": 0
                      },
                      "failure": {
                        "text": "✗ Failed",
                        "color": "red",
                        "index": 1
                      },
                      "cancelled": {
                        "text": "⊘ Cancelled",
                        "color": "yellow",
                        "index": 2
                      },
                      "skipped": {
                        "text": "○ Skipped",
                        "color": "orange",
                        "index": 3
                      }
                    }
                  }
                ]
              },
              {
                "id": "custom.width",
                "value": 120
              },
              {
                "id": "displayName",
                "value": "Status"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "repo"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 250
              },
              {
                "id": "displayName",
                "value": "Repository"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "workflow"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 200
              },
              {
                "id": "displayName",
                "value": "Workflow"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "branch"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 120
              },
              {
                "id": "displayName",
                "value": "Branch"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "duration_seconds"
            },
            "properties": [
              {
                "id": "unit",
                "value": "s"
              },
              {
                "id": "custom.width",
                "value": 100
              },
              {
                "id": "displayName",
                "value": "Duration"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "updated_at"
            },
            "properties": [
              {
                "id": "unit",
                "value": "dateTimeFromNow"
              },
              {
                "id": "custom.width",
                "value": 150
              },
              {
                "id": "displayName",
                "value": "Last Run"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "actor"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 120
              },
              {
                "id": "displayName",
                "value": "Triggered By"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "sha"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 100
              },
              {
                "id": "displayName",
                "value": "Commit"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "run_id"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 100
              },
              {
                "id": "displayName",
                "value": "Run #"
              },
              {
                "id": "links",
                "value": [
                  {
                    "title": "View Run",
                    "url": "${__data.fields.html_url}",
                    "targetBlank": true
                  }
                ]
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "html_url"
            },
            "properties": [
              {
                "id": "custom.hidden",
                "value": true
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "sha_full"
            },
            "properties": [
              {
                "id": "custom.hidden",
                "value": true
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "status"
            },
            "properties": [
              {
                "id": "custom.hidden",
                "value": true
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "event"
            },
            "properties": [
              {
                "id": "custom.hidden",
                "value": true
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "created_at"
            },
            "properties": [
              {
                "id": "custom.hidden",
                "value": true
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "run_started_at"
            },
            "properties": [
              {
                "id": "custom.hidden",
                "value": true
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "run_number"
            },
            "properties": [
              {
                "id": "custom.hidden",
                "value": true
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "^(id|tsNs|labels|Time|Line|__name__|instance|job|org)$"
            },
            "properties": [
              {
                "id": "custom.hidden",
                "value": true
              }
            ]
          }
        ]
      },
      "transformations": [
        {
          "id": "extractFields",
          "options": {
            "source": "Line"
          }
        },
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "repo": {
                "aggregations": [],
                "operation": "groupby"
              },
              "workflow": {
                "aggregations": [
                  "lastNotNull"
                ],
                "operation": "aggregate"
              },
              "conclusion": {
                "aggregations": [
                  "lastNotNull"
                ],
                "operation": "aggregate"
              },
              "branch": {
                "aggregations": [
                  "lastNotNull"
                ],
                "operation": "aggregate"
              },
              "duration_seconds": {
                "aggregations": [
                  "lastNotNull"
                ],
                "operation": "aggregate"
              },
              "updated_at": {
                "aggregations": [
                  "lastNotNull"
                ],
                "operation": "aggregate"
              },
              "actor": {
                "aggregations": [
                  "lastNotNull"
                ],
                "operation": "aggregate"
              },
              "sha": {
                "aggregations": [
                  "lastNotNull"
                ],
                "operation": "aggregate"
              },
              "run_id": {
                "aggregations": [
                  "lastNotNull"
                ],
                "operation": "aggregate"
              },
              "html_url": {
                "aggregations": [
                  "lastNotNull"
                ],
                "operation": "aggregate"
              }
            }
          }
        },
        {
          "id": "sortBy",
          "options": {
            "fields": {},
            "sort": [
              {
                "field": "updated_at (lastNotNull)",
                "desc": true
              }
            ]
          }
        },
        {
          "id": "organize",
          "options": {
            "indexByName": {
              "repo": 0,
              "workflow (lastNotNull)": 1,
              "conclusion (lastNotNull)": 2,
              "branch (lastNotNull)": 3,
              "duration_seconds (lastNotNull)": 4,
              "updated_at (lastNotNull)": 5,
              "actor (lastNotNull)": 6,
              "sha (lastNotNull)": 7,
              "run_id (lastNotNull)": 8,
              "html_url (lastNotNull)": 9
            },
            "renameByName": {
              "workflow (lastNotNull)": "workflow",
              "conclusion (lastNotNull)": "conclusion",
              "branch (lastNotNull)": "branch",
              "duration_seconds (lastNotNull)": "duration_seconds",
              "updated_at (lastNotNull)": "updated_at",
              "actor (lastNotNull)": "actor",
              "sha (lastNotNull)": "sha",
              "run_id (lastNotNull)": "run_id",
              "html_url (lastNotNull)": "html_url"
            }
          }
        }
      ],
      "options": {
        "showHeader": true,
        "cellHeight": "sm",
        "footer": {
          "show": false
        },
        "sortBy": [
          {
            "desc": true,
            "displayName": "Last Run"
          }
        ]
      }
    },
    {
      "id": 5,
      "type": "table",
      "title": "Project Health Summary",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 18
      },
      "targets": [
        {
          "expr": "sum by (repo) (count_over_time({job=\"github-actions-collector\", org=~\"$org\"} | json [$__range]))",
          "refId": "A",
          "legendFormat": "{{repo}}",
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          }
        },
        {
          "expr": "sum by (repo) (count_over_time({job=\"github-actions-collector\", org=~\"$org\"} | json | conclusion=\"success\" [$__range]))",
          "refId": "B",
          "legendFormat": "{{repo}}",
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          }
        },
        {
          "expr": "avg by (repo) (avg_over_time({job=\"github-actions-collector\", org=~\"$org\"} | json | unwrap duration_seconds [$__range]))",
          "refId": "C",
          "legendFormat": "{{repo}}",
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left",
            "cellOptions": {
              "type": "auto"
            }
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "repo"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 250
              },
              {
                "id": "displayName",
                "value": "Repository"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Value #A"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 100
              },
              {
                "id": "displayName",
                "value": "Total Runs"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Value #B"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 100
              },
              {
                "id": "displayName",
                "value": "Successes"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Success Rate"
            },
            "properties": [
              {
                "id": "unit",
                "value": "percent"
              },
              {
                "id": "custom.width",
                "value": 150
              },
              {
                "id": "custom.cellOptions",
                "value": {
                  "mode": "gradient",
                  "type": "gauge"
                }
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "value": null,
                      "color": "red"
                    },
                    {
                      "value": 80,
                      "color": "yellow"
                    },
                    {
                      "value": 95,
                      "color": "green"
                    }
                  ]
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Value #C"
            },
            "properties": [
              {
                "id": "unit",
                "value": "s"
              },
              {
                "id": "custom.width",
                "value": 120
              },
              {
                "id": "displayName",
                "value": "Avg Duration"
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "^Time.*"
            },
            "properties": [
              {
                "id": "custom.hidden",
                "value": true
              }
            ]
          }
        ]
      },
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "fieldName": "Success Rate",
            "binary": {
              "left": "Value #B",
              "reducer": "sum",
              "operator": "/",
              "right": "Value #A"
            },
            "reduce": {
              "reducer": "sum"
            },
            "replaceFields": false
          }
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "fieldName": "Success Rate",
            "binary": {
              "left": "Success Rate",
              "operator": "*",
              "right": "100"
            },
            "replaceFields": false
          }
        },
        {
          "id": "sortBy",
          "options": {
            "fields": {},
            "sort": [
              {
                "field": "Success Rate",
                "desc": false
              }
            ]
          }
        }
      ],
      "options": {
        "showHeader": true,
        "cellHeight": "sm",
        "footer": {
          "show": false
        },
        "sortBy": [
          {
            "desc": false,
            "displayName": "Success Rate"
          }
        ]
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Workflow Runs Over Time",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 28
      },
      "targets": [
        {
          "expr": "sum by (conclusion) (count_over_time({job=\"github-actions-collector\", org=~\"$org\"} | json [$__interval]))",
          "refId": "A",
          "legendFormat": "{{conclusion}}",
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80,
            "stacking": {
              "mode": "normal",
              "group": "A"
            },
            "showPoints": "never"
          },
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "success"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "failure"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "cancelled"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "orange"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "calcs": [
            "sum"
          ]
        }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "loki",
        "current": {
          "text": "grafanacloud-logs",
          "value": "grafanacloud-logs"
        }
      },
      {
        "name": "org",
        "type": "query",
        "label": "Organization",
        "query": "label_values({job=\"github-actions-collector\"}, org)",
        "datasource": {
          "type": "loki",
          "uid": "${datasource}"
        },
        "multi": true,
        "includeAll": true,
        "allValue": ".+"
      }
    ]
  },
  "annotations": {
    "list": []
  },
  "refresh": "5m",
  "time": {
    "from": "now-30d",
    "to": "now"
  },
  "timepicker": {}
}
